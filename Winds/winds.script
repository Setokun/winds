CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE SCORES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,IDPLAYER INTEGER,IDLEVEL INTEGER,TIME INTEGER,NBCLICKS INTEGER,NBITEMS INTEGER,RANK INTEGER)
CREATE MEMORY TABLE LEVELS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR,DESCRIPTION VARCHAR,CREATIONDATE TIMESTAMP,TIMEMAX INTEGER,LEVELTYPE VARCHAR,LEVELSTATUS VARCHAR,LEVELMODE VARCHAR,IDCREATOR INTEGER,IDTHEME INTEGER)
CREATE MEMORY TABLE THEMES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR,DESCRIPTION VARCHAR,CREATIONDATE TIMESTAMP,IDCREATOR INTEGER)
CREATE MEMORY TABLE USERS(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,EMAIL VARCHAR,PASSWORD VARCHAR,PSEUDO VARCHAR,REGISTRATIONDATE VARCHAR,USERTYPE VARCHAR,CURRENT BOOLEAN)
CREATE MEMORY TABLE TROPHIES(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,IDPLAYER INTEGER,DESCRIPTION VARCHAR,ACHIEVED VARCHAR)
ALTER TABLE SCORES ALTER COLUMN ID RESTART WITH 21
ALTER TABLE LEVELS ALTER COLUMN ID RESTART WITH 21
ALTER TABLE THEMES ALTER COLUMN ID RESTART WITH 4
ALTER TABLE USERS ALTER COLUMN ID RESTART WITH 6
ALTER TABLE TROPHIES ALTER COLUMN ID RESTART WITH 4
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
SET SCHEMA PUBLIC
